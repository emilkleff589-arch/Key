local function GetIcon(Name) return Icons[Name] or Icons.Home end

local function Tween(Instance, Properties, Duration, Style, Direction) local Info = TweenInfo.new(Duration or 0.4, Style or Enum.EasingStyle.Quint, Direction or Enum.EasingDirection.Out) local T = TweenService:Create(Instance, Info, Properties) T:Play() return T end

local function MakeDraggable(TopFrame, MainFrame) local Dragging, DragInput, DragStart, StartPos TopFrame.InputBegan:Connect(function(Input) if Input.UserInputType == Enum.UserInputType.MouseButton1 then Dragging = true DragStart = Input.Position StartPos = MainFrame.Position Input.Changed:Connect(function() if Input.UserInputState == Enum.UserInputState.End then Dragging = false end end) end end) UserInputService.InputChanged:Connect(function(Input) if Dragging and Input.UserInputType == Enum.UserInputType.MouseMovement then local Delta = Input.Position - DragStart MainFrame.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + Delta.X, StartPos.Y.Scale, StartPos.Y.Offset + Delta.Y) end end) end

local function MakeResizable(MainFrame, Handle) local Dragging, DragStart, StartSize Handle.InputBegan:Connect(function(Input) if Input.UserInputType == Enum.UserInputType.MouseButton1 then Dragging = true DragStart = Input.Position StartSize = MainFrame.AbsoluteSize Input.Changed:Connect(function() if Input.UserInputState == Enum.UserInputState.End then Dragging = false end end) end end) UserInputService.InputChanged:Connect(function(Input) if Dragging and Input.UserInputType == Enum.UserInputType.MouseMovement then local Delta = Input.Position - DragStart MainFrame.Size = UDim2.new(0, math.clamp(StartSize.X + Delta.X, 500, 1000), 0, math.clamp(StartSize.Y + Delta.Y, 350, 700)) end end) end

