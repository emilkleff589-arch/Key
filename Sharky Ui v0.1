    Main.Visible = false
    local KMain = Instance.new("Frame", ScreenGui)
    KMain.Size = UDim2.new(0, 300, 0, 180)
    KMain.Position = UDim2.new(0.5, -150, 0.5, -90)
    KMain.BackgroundColor3 = Library.Theme.Main
    Instance.new("UICorner", KMain).CornerRadius = UDim.new(0, 10)
    
    local KInput = Instance.new("TextBox", KMain)
    KInput.Size = UDim2.new(0.8, 0, 0, 35)
    KInput.Position = UDim2.new(0.1, 0, 0.4, 0)
    KInput.PlaceholderText = "Enter License Key"
    KInput.BackgroundColor3 = Library.Theme.Content
    KInput.TextColor3 = Color3.new(1,1,1)
    Instance.new("UICorner", KInput)

    local KBtn = Instance.new("TextButton", KMain)
    KBtn.Size = UDim2.new(0.8, 0, 0, 35)
    KBtn.Position = UDim2.new(0.1, 0, 0.7, 0)
    KBtn.Text = "Verify"
    KBtn.BackgroundColor3 = Library.Theme.Accent
    Instance.new("UICorner", KBtn)
    
    KBtn.MouseButton1Click:Connect(function()
        if KInput.Text == KConfig.Key then
            KMain:Destroy()
            Main.Visible = true
        end
    end)
end

function Library:Notify(NConfig)
    local N = Instance.new("Frame", ScreenGui)
    N.Size = UDim2.new(0, 250, 0, 60)
    N.Position = UDim2.new(1, 10, 1, -70)
    N.BackgroundColor3 = Library.Theme.Sidebar
    Instance.new("UICorner", N).CornerRadius = UDim.new(0, 8)
    Instance.new("UIStroke", N).Color = Library.Theme.Accent
    
    local NT = Instance.new("TextLabel", N)
    NT.Text = NConfig.Title
    NT.Font = Enum.Font.GothamBold
    NT.Position = UDim2.new(0, 10, 0, 10)
    NT.TextColor3 = Library.Theme.Accent
    NT.BackgroundTransparency = 1
    
    local NC = Instance.new("TextLabel", N)
    NC.Text = NConfig.Content
    NC.Position = UDim2.new(0, 10, 0, 30)
    NC.TextColor3 = Library.Theme.Text
    NC.BackgroundTransparency = 1
    
    Tween(N, {Position = UDim2.new(1, -260, 1, -70)}, 0.5)
    task.delay(NConfig.Duration or 3, function()
        Tween(N, {Position = UDim2.new(1, 10, 1, -70)}, 0.5)
        task.wait(0.5) N:Destroy()
    end)
end

return Window
end

return Library
