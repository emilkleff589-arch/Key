local Tabs = {}
local Window = {}

function Window:CreateTab(TabConfig)
    local TabBtn = Instance.new("TextButton", Sidebar)
    TabBtn.Size = UDim2.new(1, -12, 0, 35)
    TabBtn.BackgroundTransparency = 1
    TabBtn.Text = ""
    TabBtn.AutoButtonColor = false
    
    local TCorner = Instance.new("UICorner", TabBtn)
    TCorner.CornerRadius = UDim.new(0, 6)

    local TIcon = Instance.new("ImageLabel", TabBtn)
    TIcon.Size = UDim2.new(0, 18, 0, 18)
    TIcon.Position = UDim2.new(0, 10, 0.5, -9)
    TIcon.Image = GetIcon(TabConfig.Icon)
    TIcon.ImageColor3 = Library.Theme.SubText
    TIcon.BackgroundTransparency = 1

    local TText = Instance.new("TextLabel", TabBtn)
    TText.Position = UDim2.new(0, 38, 0, 0)
    TText.Size = UDim2.new(1, -40, 1, 0)
    TText.Text = TabConfig.Name
    TText.Font = Enum.Font.GothamMedium
    TText.TextColor3 = Library.Theme.SubText
    TText.TextSize = 13
    TText.BackgroundTransparency = 1
    TText.TextXAlignment = Enum.TextXAlignment.Left

    local Page = Instance.new("ScrollingFrame", ContentArea)
    Page.Size = UDim2.new(1, 0, 1, 0)
    Page.BackgroundTransparency = 1
    Page.Visible = false
    Page.ScrollBarThickness = 2
    Page.ScrollBarImageColor3 = Library.Theme.Accent
    
    local PLayout = Instance.new("UIListLayout", Page)
    PLayout.Padding = UDim.new(0, 10)
    Instance.new("UIPadding", Page).PaddingRight = UDim.new(0, 5)

    local function Select()
        for _, t in pairs(Tabs) do
            t.Page.Visible = false
            Tween(t.Btn, {BackgroundTransparency = 1}, 0.3)
            Tween(t.Text, {TextColor3 = Library.Theme.SubText}, 0.3)
            Tween(t.Icon, {ImageColor3 = Library.Theme.SubText}, 0.3)
        end
        Page.Visible = true
        TabIndicator.Visible = true
        Tween(TabIndicator, {Position = UDim2.new(0, -5, 0, TabBtn.Position.Y + 8)}, 0.4, Enum.EasingStyle.Expo)
        Tween(TabBtn, {BackgroundTransparency = 0.9}, 0.3)
        Tween(TText, {TextColor3 = Library.Theme.Text}, 0.3)
        Tween(TIcon, {ImageColor3 = Library.Theme.Accent}, 0.3)
        
        -- Bounce effect
        TIcon.Size = UDim2.new(0, 22, 0, 22)
        TIcon.Position = UDim2.new(0, 8, 0.5, -11)
        Tween(TIcon, {Size = UDim2.new(0, 18, 0, 18), Position = UDim2.new(0, 10, 0.5, -9)}, 0.5, Enum.EasingStyle.Back)
    end

    TabBtn.MouseButton1Click:Connect(Select)
    table.insert(Tabs, {Btn = TabBtn, Page = Page, Text = TText, Icon = TIcon})

    if #Tabs == 1 then Select() end

    local Sections = {}
    function Sections:CreateSection(Name)
        local SFrame = Instance.new("Frame", Page)
        SFrame.Size = UDim2.new(1, 0, 0, 40)
        SFrame.BackgroundColor3 = Library.Theme.Main
        SFrame.BackgroundTransparency = 0.4
        Instance.new("UICorner", SFrame).CornerRadius = UDim.new(0, 8)
        Instance.new("UIStroke", SFrame).Color = Library.Theme.Stroke
        
        local STitle = Instance.new("TextLabel", SFrame)
        STitle.Text = Name:upper()
        STitle.Font = Enum.Font.GothamBold
        STitle.TextSize = 11
        STitle.TextColor3 = Library.Theme.Accent
        STitle.Position = UDim2.new(0, 12, 0, 10)
        STitle.BackgroundTransparency = 1
        
        local Container = Instance.new("Frame", SFrame)
        Container.Position = UDim2.new(0, 10, 0, 30)
        Container.Size = UDim2.new(1, -20, 0, 0)
        Container.BackgroundTransparency = 1
        local CLayout = Instance.new("UIListLayout", Container)
        CLayout.Padding = UDim.new(0, 6)

        CLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            SFrame.Size = UDim2.new(1, 0, 0, CLayout.AbsoluteContentSize.Y + 40)
            Page.CanvasSize = UDim2.new(0,0,0, PLayout.AbsoluteContentSize.Y + 20)
        end)

        local Elements = {}
        function Elements:CreateButton(EConfig)
            local B = Instance.new("TextButton", Container)
            B.Size = UDim2.new(1, 0, 0, 32)
            B.BackgroundColor3 = Library.Theme.Content
            B.Text = EConfig.Name
            B.Font = Enum.Font.Gotham
            B.TextColor3 = Library.Theme.Text
            B.TextSize = 12
            Instance.new("UICorner", B).CornerRadius = UDim.new(0, 6)
            B.MouseButton1Click:Connect(function()
                Tween(B, {BackgroundColor3 = Library.Theme.Accent}, 0.1)
                task.wait(0.1)
                Tween(B, {BackgroundColor3 = Library.Theme.Content}, 0.2)
                EConfig.Callback()
            end)
        end

        function Elements:CreateToggle(EConfig)
            local T = Instance.new("TextButton", Container)
            T.Size = UDim2.new(1, 0, 0, 32)
            T.BackgroundColor3 = Library.Theme.Content
            T.Text = ""
            Instance.new("UICorner", T).CornerRadius = UDim.new(0, 6)
            
            local L = Instance.new("TextLabel", T)
            L.Text = EConfig.Name
            L.Position = UDim2.new(0, 12, 0, 0)
            L.Size = UDim2.new(1, 0, 1, 0)
            L.Font = Enum.Font.Gotham
            L.TextColor3 = Library.Theme.Text
            L.TextSize = 12
            L.BackgroundTransparency = 1
            L.TextXAlignment = Enum.TextXAlignment.Left

            local Box = Instance.new("Frame", T)
            Box.Size = UDim2.new(0, 34, 0, 18)
            Box.Position = UDim2.new(1, -44, 0.5, -9)
            Box.BackgroundColor3 = Color3.fromRGB(60,60,60)
            Instance.new("UICorner", Box).CornerRadius = UDim.new(1, 0)
            
            local Dot = Instance.new("Frame", Box)
            Dot.Size = UDim2.new(0, 14, 0, 14)
            Dot.Position = UDim2.new(0, 2, 0.5, -7)
            Dot.BackgroundColor3 = Color3.fromRGB(255,255,255)
            Instance.new("UICorner", Dot).CornerRadius = UDim.new(1, 0)

            local Enabled = EConfig.CurrentValue or false
            local function Update()
                Tween(Box, {BackgroundColor3 = Enabled and Library.Theme.Accent or Color3.fromRGB(60,60,60)}, 0.3)
                Tween(Dot, {Position = Enabled and UDim2.new(1, -16, 0.5, -7) or UDim2.new(0, 2, 0.5, -7)}, 0.3)
                EConfig.Callback(Enabled)
            end
            T.MouseButton1Click:Connect(function() Enabled = not Enabled Update() end)
            Update()
        end

        return Elements
    end
    return Sections
end

